{
  "name": "Stoplight Status Visualization",
  "organization": "Your Organization Name",
  "description": "A stoplight chart showing project status based on executed and total population.",
  "logoUrl": "",
  "packageUrl": "https://yourdomain.com/package-info",
  "organizationUrl": "",
  "supportUrl": "",
  "privacyPolicyUrl": "",
  "termsOfServiceUrl": "",
  "devMode": false,
  "components": [
    {
      "id": "stoplightStatusViz",
      "name": "Stoplight Status Visualization",
      "description": "A stoplight chart showing project status based on executed and total population.",
      "iconUrl": "https://yourdomain.com/path/to/icon.png",
      "infoUrl": "https://yourdomain.com/visualization-info",
      "resource": {
        "js": "https://yourdomain.com/path/to/script.js",
        "config": "https://yourdomain.com/path/to/manifest.json",
        "css": "https://yourdomain.com/path/to/style.css"
      }
    }
  ],
  "data": {
    "required": [
      {
        "name": "Total Population",
        "type": "METRIC"
      },
      {
        "name": "Executed Population",
        "type": "METRIC"
      }
    ],
    "optional": [
      {
        "name": "Project Name",
        "type": "DIMENSION"
      }
    ]
  },
  "style": {
    "redThreshold": {
      "defaultValue": 0.3,
      "type": "NUMBER",
      "description": "Threshold for Red status (percentage)"
    },
    "yellowThreshold": {
      "defaultValue": 0.7,
      "type": "NUMBER",
      "description": "Threshold for Yellow status (percentage)"
    },
    "greenThreshold": {
      "defaultValue": 1,
      "type": "NUMBER",
      "description": "Threshold for Green status (percentage)"
    },
    "color": {
      "defaultValue": {
        "color": "#000000",
        "opacity": 1
      }
    }
  }
}
